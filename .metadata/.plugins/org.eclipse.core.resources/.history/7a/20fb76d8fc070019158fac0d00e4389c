package edu.yctc.genesis.dao;

import org.apache.ibatis.annotations.Mapper;

import edu.yctc.genesis.entity.Class_scoreDO;

@Mapper
public interface Class_scoreDAO {
	/**
	 * 插入一条课堂评分记录
	 * 
	 * @param lessonDO
	 */
	
	public void insert(Class_scoreDO class_scoreDO);
	/**
	 * 用户id查询考勤情况
	 * 
	 * @param  id
	 * @return
	 */
	public Class_scoreDO getScoreByUserId(long id);
}
