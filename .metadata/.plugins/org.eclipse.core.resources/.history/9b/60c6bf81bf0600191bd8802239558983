package edu.yctc.genesis.entity;

public class permission_group {
	/** 权限id*/
	private long   id;
	/** 权限名*/
	private String name;
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	@Override
	public String toString() {
		return "permission [id=" + id + ", name=" + name + "]";
	}
}
