package edu.yctc.genesis.dao;

import org.apache.ibatis.annotations.Mapper;

import edu.yctc.genesis.entity.AttendanceDO;

@Mapper
public interface Class_scoreDAO {
	/**
	 * 插入一条课堂评分记录
	 * 
	 * @param lessonDO
	 */
	
	public void insert(Class_scoreDAO class_scoreDAO);
	
	/**
	 * 用户id查询考勤情况
	 * 
	 * @param  id
	 * @return
	 */
	public AttendanceDO getAttendacneByUserId(long id);
}
