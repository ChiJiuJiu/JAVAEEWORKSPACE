package edu.yctc.genesis.dao;

import org.apache.ibatis.annotations.Mapper;

import edu.yctc.genesis.entity.Classroom_stateDO;


@Mapper
public interface Classroom_stateDAO {
	/**
	 * 插入一条课堂评分记录
	 * 
	 * @param lessonDO
	 */
	
	public void insert(Classroom_stateDO classroom_stateDO);
	/**
	 * 用户id查询考勤情况
	 * 
	 * @param  id
	 * @return
	 */
	public Classroom_stateDO getStateByClassroomId(long id);
}
