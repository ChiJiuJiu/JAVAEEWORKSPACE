package edu.yctc.genesis.entity;

public class attendance {
	/** 主键*/
	private long id;
	/** 用户id*/
	private long user_id;
	/** 课时id*/
	private long lesson_id;
	/** 出勤情况*/
	private int  attend_state;
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public long getUser_id() {
		return user_id;
	}
	public void setUser_id(long user_id) {
		this.user_id = user_id;
	}
	public long getLesson_id() {
		return lesson_id;
	}
	public void setLesson_id(long lesson_id) {
		this.lesson_id = lesson_id;
	}
	public int getAttend_state() {
		return attend_state;
	}
	public void setAttend_state(int attend_state) {
		this.attend_state = attend_state;
	}
	@Override
	public String toString() {
		return "attendance [id=" + id + ", user_id=" + user_id + ", lesson_id=" + lesson_id + ", attend_state="
				+ attend_state + "]";
	}
	
}
